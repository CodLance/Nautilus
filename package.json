{
  "name": "nautilus",
  "author": "Mitch Pierias",
  "description": "EOS Account and Contract Management",
  "repository": "https://github.com/MitchPierias/Nautilus",
  "version": "0.0.1",
  "private": false,
  "main": "public/main.js",
  "homepage": "./",
  "dependencies": {
    "chokidar": "^2.0.4",
    "electron-is-dev": "^0.3.0",
    "eosjs": "^16.0.7",
    "path": "^0.12.7",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-scripts": "1.1.5",
    "url": "^0.11.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron ~/Contracts/nautilus\"",
    "preelectron-pack": "yarn build",
    "electron-pack": "build --em.main=build/main.js"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "electron": "^2.0.9",
    "electron-builder": "^20.28.4",
    "electron-rebuild": "^1.8.2",
    "wait-on": "^3.0.1"
  },
  "build": {
    "appId": "com.example.nautilus",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
